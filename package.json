{
  "name": "stations",
  "version": "0.0.0",
  "scripts": {
    "build:app": "ng build",
    "build:lambda": "netlify-lambda build ./src/lambda/src",
    "build:prod": "ng build --prod && npm run build:lambda",
    "lint:app": "ng lint",
    "lint:lambda": "eslint ./src/lambda/src/**/*.js",
    "lint": "npm run lint:app && npm run lint:lambda",
    "ng": "ng",
    "postinstall": "ngcc && netlify-lambda install ./src/lambda",
    "serve:mock-data": "json-server --watch ./mock/data/mock-data.json --routes ./mock/routes.json",
    "start": "concurrently \"npm run start:app\" \"npm run start:lambda\"",
    "start:app": "ng serve",
    "start:mock": "concurrently --kill-others \"npm run serve:mock-data\" \"npm run start:app -- --configuration=mock\"",
    "start:lambda": "netlify-lambda serve ./src/lambda/src",
    "test": "jest",
    "test:coverage": "npm test -- --coverage",
    "test:watch": "npm test -- --watch"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint && npm test"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.0.5",
    "@angular/cdk": "^11.0.3",
    "@angular/common": "^11.0.5",
    "@angular/compiler": "^11.0.5",
    "@angular/core": "^11.0.5",
    "@angular/forms": "^11.0.5",
    "@angular/material": "~11.0.3",
    "@angular/platform-browser": "^11.0.5",
    "@angular/platform-browser-dynamic": "^11.0.5",
    "@angular/router": "^11.0.5",
    "@angular/service-worker": "^11.0.5",
    "netlify-lambda": "^2.0.2",
    "ngx-mat-select-search": "^3.2.0",
    "rxjs": "~6.6.3",
    "tslib": "^2.0.3",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.803.21",
    "@angular-devkit/build-angular": "^0.1100.5",
    "@angular-devkit/core": "^11.0.5",
    "@angular-devkit/schematics": "^11.0.5",
    "@angular/cli": "~11.0.5",
    "@angular/compiler-cli": "^11.0.5",
    "@angular/language-service": "^11.0.5",
    "@angular-eslint/builder": "0.8.0-beta.7",
    "@angular-eslint/eslint-plugin": "0.8.0-beta.7",
    "@angular-eslint/eslint-plugin-template": "0.8.0-beta.7",
    "@angular-eslint/schematics": "^0.8.0-beta.7",
    "@angular-eslint/template-parser": "0.8.0-beta.7",
    "@types/jest": "~26.0.19",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
    "concurrently": "^5.3.0",
    "eslint": "^7.16.0",
    "husky": "~4.3.6",
    "jest": "~26.6.3",
    "jest-localstorage-mock": "^2.4.6",
    "jest-preset-angular": "~8.3.2",
    "json-server": "^0.16.3",
    "ts-jest": "^26.4.4",
    "ts-node": "~9.1.1",
    "typescript": "4.0.5"
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!@ngrx|angular2-ui-switch|ng-dynamic)"
    ],
    "coverageDirectory": "./coverage",
    "coverageReporters": [
      "text",
      "html"
    ],
    "reporters": [
      "default"
    ],
    "collectCoverageFrom": [
      "src/api/**/**.ts",
      "src/core/**/**.ts",
      "src/modules/**/**.ts",
      "src/lambda/**/**.js",
      "!**.*.d.ts",
      "!src/**/*.module.ts",
      "!src/core/**/*-routing.module.ts",
      "!src/lambda/**/config.js",
      "!src/lambda/**/constants.js",
      "!src/lambda/**/*-constants.js"
    ],
    "roots": [
      "src"
    ],
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "./setup-jest.ts"
    ],
    "setupFiles": [
      "jest-localstorage-mock"
    ],
    "globals": {
      "ts-jest": {
        "tsconfig": "./tsconfig.test.json",
        "ignoreCoverageForAllDecorators": true,
        "stringifyContentPathRegex": "\\.html$",
        "astTransformers": [
          "jest-preset-angular/build/InlineFilesTransformer",
          "jest-preset-angular/build/StripStylesTransformer"
        ]
      }
    }
  }
}
